<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SysAdmin">
   <children>
      <TextField fx:id="userTypeTextField" layoutX="26.0" layoutY="31.0" promptText="User Type" />
      <TextField fx:id="passwordTextField" layoutX="26.0" layoutY="131.0" promptText="Password" />
      <TextField fx:id="usernameTextField" layoutX="26.0" layoutY="106.0" promptText="Username" />
      <TextField fx:id="emailTextField" layoutX="26.0" layoutY="81.0" promptText="Email" />
      <TextField fx:id="phoneNumberTextField" layoutX="26.0" layoutY="56.0" promptText="Phone Number" />
      <Button layoutX="208.0" layoutY="131.0" mnemonicParsing="false" onAction="#createAccountButton" text="Create Account" />
      <TextField fx:id="deleteUserTextField" layoutX="26.0" layoutY="200.0" promptText="Username" />
      <Button layoutX="207.0" layoutY="200.0" mnemonicParsing="false" onAction="#deleteAccountButton" prefHeight="27.0" prefWidth="114.0" text="Delete Account" />
      <TextField fx:id="resetUserTextField" layoutX="26.0" layoutY="268.0" promptText="Username" />
      <TextField fx:id="resetPasswordTextField" layoutX="26.0" layoutY="295.0" promptText="New Pasword" />
      <Button layoutX="203.0" layoutY="295.0" mnemonicParsing="false" onAction="#resetPasswordButton" prefHeight="27.0" prefWidth="123.0" text="Reset Password" />
   </children>
